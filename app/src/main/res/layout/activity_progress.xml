<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.chris.strokere.Progress">

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:background="@color/bg"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TableRow
                android:layout_width="match_parent"
                android:layout_marginTop="130dp"
                android:layout_height="match_parent"
                android:gravity="left">

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center">

                <TextView
                    android:id="@+id/d01P"
                    style="@style/calendar_day"
                    android:background="@drawable/calendar_day"
                    android:text="1" />

                <TextView
                    android:id="@+id/d02P"
                    style="@style/calendar_day"
                    android:background="@drawable/calendar_day"
                    android:text="2" />

                <TextView
                    android:id="@+id/d03P"
                    style="@style/calendar_day"
                    android:background="@drawable/calendar_day"
                    android:text="3" />

                <TextView
                    android:id="@+id/d04P"
                    style="@style/calendar_day"
                    android:background="@drawable/calendar_day"
                    android:text="4" />

                <TextView
                    android:id="@+id/d05P"
                    style="@style/calendar_day"
                    android:background="@drawable/calendar_day"
                    android:text="5" />

                <TextView
                    android:id="@+id/d06P"
                    style="@style/calendar_day"
                    android:background="@drawable/calendar_day"
                    android:text="6" />

                <TextView
                    android:id="@+id/d07P"
                    style="@style/calendar_day"
                    android:background="@drawable/calendar_day"
                    android:text="7" />


            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center">

                <TextView
                    android:id="@+id/d08P"
                    style="@style/calendar_day"
                    android:background="@drawable/calendar_day"
                    android:text="8" />

                <TextView
                    android:id="@+id/d09P"
                    style="@style/calendar_day"
                    android:background="@drawable/calendar_day"
                    android:text="9" />

                <TextView
                    android:id="@+id/d10P"
                    style="@style/calendar_day"
                    android:background="@drawable/calendar_day"
                    android:text="10" />

                <TextView
                    android:id="@+id/d11P"
                    style="@style/calendar_day"
                    android:background="@drawable/calendar_day"
                    android:text="11" />

                <TextView
                    android:id="@+id/d12P"
                    style="@style/calendar_day"
                    android:background="@drawable/calendar_day"
                    android:text="12" />

                <TextView
                    android:id="@+id/d13P"
                    style="@style/calendar_day"
                    android:background="@drawable/calendar_day"
                    android:text="13" />

                <TextView
                    android:id="@+id/d14P"
                    style="@style/calendar_day"
                    android:background="@drawable/calendar_day"
                    android:text="14" />

            </TableRow>

            <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">

            <TextView
                android:id="@+id/d15P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text="15" />

            <TextView
                android:id="@+id/d16P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text="16" />

            <TextView
                android:id="@+id/d17P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text="17" />

            <TextView
                android:id="@+id/d18P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text="18" />

            <TextView
                android:id="@+id/d19P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text="19" />

            <TextView
                android:id="@+id/d20P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text="20" />

            <TextView
                android:id="@+id/d21P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text="21" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">

            <TextView
                android:id="@+id/d22P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text="22" />

            <TextView
                android:id="@+id/d23P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text="23" />

            <TextView
                android:id="@+id/d24P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text="24" />

            <TextView
                android:id="@+id/d25P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text="25" />

            <TextView
                android:id="@+id/d26P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text="26" />

            <TextView
                android:id="@+id/d27P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text="27" />

            <TextView
                android:id="@+id/d28P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text="28" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">

            <TextView
                android:id="@+id/d29P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text="29" />

            <TextView
                android:id="@+id/d30P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text="30" />

            <TextView
                android:id="@+id/d31P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text="31" />

            <TextView
                android:id="@+id/dEmpty4P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text=""
                android:visibility="invisible"/>

            <TextView
                android:id="@+id/dEmpty5P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text=""
                android:visibility="invisible"/>

            <TextView
                android:id="@+id/dEmpty6"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text=""
                android:visibility="invisible"/>

            <TextView
                android:id="@+id/dEmpty7P"
                style="@style/calendar_day"
                android:background="@drawable/calendar_day"
                android:text=""
                android:visibility="invisible"/>

        </TableRow>


        </TableLayout>

        <TextView
            android:id="@+id/currentMonth"
            android:layout_width="120dp"
            android:gravity="center"
            android:layout_height="wrap_content"
            android:layout_below="@+id/include4"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="24dp"
            android:text=""
            android:textColor="@color/colorPrimary"
            android:textColorLink="@color/colorPrimary"
            android:textSize="24dp" />

        <include layout="@layout/navbar"
            android:id="@+id/include4" />

        <Button
            android:id="@+id/shuffle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="19dp"
            android:text="Shuffle" />

        <Button
            android:id="@+id/goGraph"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/shuffle"
            android:layout_alignBottom="@+id/shuffle"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="14dp"
            android:text="graph" />

        <Button
            android:id="@+id/backMonth"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/currentMonth"
            android:layout_alignBottom="@+id/currentMonth"
            android:layout_marginEnd="16dp"
            android:layout_toStartOf="@+id/currentMonth"
            android:background="@drawable/ic_keyboard_arrow_left_white_24dp" />

        <Button
            android:id="@+id/forwardMonth"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/currentMonth"
            android:layout_alignBottom="@+id/currentMonth"
            android:layout_marginStart="16dp"
            android:layout_toEndOf="@+id/currentMonth"
            android:background="@drawable/ic_keyboard_arrow_right_white_24dp" />

        <TextView
            android:id="@+id/workedBackground"
            android:layout_width="360dp"
            android:layout_height="55dp"
            android:background="@color/bg"
            android:layout_alignBottom="@+id/shuffle"
            android:layout_marginBottom="70dp"
            android:layout_centerHorizontal="true" />

        <TextView
            android:id="@+id/youWorked"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="20dp"
            android:textColor="@color/greenAccent"
            android:layout_alignBottom="@+id/shuffle"
            android:layout_marginBottom="120dp"
            android:layout_centerHorizontal="true" />


        <ImageView
            android:id="@+id/runningMan"
            android:layout_width="24dp"
            android:layout_height="24dp"
            app:srcCompat="@drawable/ic_directions_walk_white_24dp"
            android:layout_above="@+id/youWorked"
            android:layout_alignStart="@+id/youWorked"
            android:visibility="gone"/>

    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
